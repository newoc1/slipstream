{
  "background": {
    "scripts": ["background.js"],
    "persistent": false
  },
  "manifest_version": 2,

  "name": "Slipstream",
  "description": "This extension allows the user to manage their tabs in a 3d environment",
  "version": "1.0",

  "browser_action": {
    "default_icon": "icon.png"  
  },
  "options_ui": {
    "page":"options.html",
    "chrome_style":true
  },
  "permissions": [
    "activeTab",
    "storage",
    "tabs"
  ]  
}