{
  "manifest_version": 2,

  "name": "Slipstream",
  "description": "This extension allows the user to manage their tabs in a 3d environment",
  "version": "1.0",

  "browser_action": {
    "default_icon": "icon.png"  
  },
  "permissions": [
    "activeTab",
    "storage",
    "tabs"
  ],
  "chrome_url_overrides": {
    "newtab": "main.html"
  }
}